<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="199.0" prefWidth="196.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="usernameLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
   <TextField fx:id="usernameTextField" promptText="Username" />
   <Text fx:id="passwordLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
   <TextField fx:id="passwordTextField" onAction="#onLoginButtonClick" promptText="Password" />

    <Label fx:id="welcomeText" />
    <Button fx:id="loginButton" onAction="#onLoginButtonClick" text="Login" />
</VBox>
